.container
  .wrapper.col0
    #topbar
      #slidepanel
        .topbox
          p.readmore1
            a(href='guide') 查看网站指南»»»
          h2 管理员信息
          p 所做操作，拥有权限，身份信息
        .topbox
          h2 修改密码
          form(action='#', method='post')
            fieldset
              legend Teachers Login Form
              label(for='teachername')
                | 输入新密码：
                input#teachername(type='text', name='teachername', value='')
              label(for='teacherpass')
                | 确认密码：
                input#teacherpass(type='password', name='teacherpass', value='')
              p
                input#teacherlogin(type='submit', name='teacherlogin', value='修改')
                input#teacherreset(type='reset', name='teacherreset', value='清空')
        .topbox.last
          h2 切换用户
          form(action='#', method='post')
            fieldset
              legend Pupils Login Form
              label(for='pupilname')
                | 帐号：
                input#pupilname(type='text', name='pupilname', value='')
              label(for='pupilpass')
                | 密码：
                input#pupilpass(type='password', name='pupilpass', value='')
              label(for='pupilremember')
                input#pupilremember.checkbox(type='checkbox', name='pupilremember', checked='checked')
              p
                input#pupillogin(type='submit', name='pupillogin', value='登录')
                input#pupilreset(type='reset', name='pupilreset', value='注销')
        br.clear
      #loginpanel
        ul
          li.left 不懂看这里»
          li#toggle.right
            a#slideit(href='#slidepanel') 我的信息
            a#closeit(style='display: none;', href='#slidepanel') 关闭
      br.clear
